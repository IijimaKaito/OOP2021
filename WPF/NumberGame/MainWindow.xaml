<Window x:Name="MainForm" x:Class="NumberGame.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NumberGame"
        mc:Ignorable="d"
        Title="数あてゲーム" Height="800" Width="800" Loaded="Window_Loaded">
    <StackPanel>
        <Grid x:Name="gridPanel">
            <TextBlock x:Name="answerText" Height="30" FontSize="15" TextWrapping="Wrap" Text="TextBlock" Margin="4,740,-4.4,-739.6" />
        </Grid>
    </StackPanel>
</Window>
